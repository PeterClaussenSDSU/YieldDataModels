@article{staswick-08-1989,
	abstract = {A temporary vegetative storage protein, composed of similar 25 kilodalton and 27 kilodalton subunits, was found to be abundant in soybean (Glycine max (L.) Herr. var Hobbit) leaves, stems, pods, flower petals, germinated cotyledons, and less abundant in roots, nodules and seeds. Total pod protein was highest at 3 weeks after flowering and declined by 37% within 3 weeks during seed development. During this time the vegetative storage protein declined from 18% to 1.5% of the total pod protein and accounted for 45% of the protein lost from pods. This indicates that the vegetative storage protein makes a significant contribution to the pool of nutrients mobilized from pods for transport to developing seeds.},
	affiliation = {Department of Agronomy, University of Nebraska, Lincoln, Nebraska 68583-0915.},
	annote = {At 2 weeks after flowering, seeds were less than 10 mg; at 7 WAF seeds and pods were starting to yellow. Protein (total) pod levels peaked at 3 WAF ({\textasciitilde}11 mg/pod), to {\textasciitilde}7 at 6 WAF and {\textasciitilde}3 at {\&}amp; WAF; seed protein was {\textasciitilde}4 at 3 WAF, {\textasciitilde}14 at 6 WAF and 12 at 7 WAF. VSP was 12 at 2 WAF, 18% at 3 WAF, dropped to near 0 by 6 WAF.},
